<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.image.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<FlowPane prefHeight="205.0" prefWidth="317.0" stylesheets="@Style.css" xmlns="http://javafx.com/javafx/17.0.12" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.esand.gerenciamentorh.controller.LoginController">
    <children>
      <VBox alignment="CENTER" prefHeight="320.0" prefWidth="315.0" spacing="20.0" style="-fx-background-color: white;">
         <children>
            <ImageView fitHeight="71.0" fitWidth="71.0" pickOnBounds="true">
               <image>
                  <Image url="@../../../../imagens/monitor.png" />
               </image>
            </ImageView>
            <VBox maxWidth="220.0" prefHeight="111.0" prefWidth="200.0">
               <children>
                  <Label prefHeight="17.0" prefWidth="23.0" text="CPF">
                     <font>
                        <Font name="JetBrains Mono Regular" size="12.0" />
                     </font></Label>
                  <TextField fx:id="CPF" styleClass="caixa-texto" />
                  <Label text="Senha">
                     <font>
                        <Font name="JetBrains Mono Regular" size="12.0" />
                     </font>
                     <VBox.margin>
                        <Insets top="20.0" />
                     </VBox.margin></Label>
                  <PasswordField fx:id="Senha" styleClass="caixa-texto" />
               </children>
            </VBox>
            <Button id="acesso" mnemonicParsing="false" onAction="#logar" prefHeight="30.0" prefWidth="85.0" styleClass="botao" text="Acessar" textAlignment="CENTER" textFill="WHITE" textOverrun="WORD_ELLIPSIS">
               <font>
                  <Font name="JetBrains Mono Medium" size="12.0" />
               </font>
               <VBox.margin>
                  <Insets />
               </VBox.margin></Button>
         </children>
         <padding>
            <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
         </padding>
      </VBox>
    </children>
   <opaqueInsets>
      <Insets />
   </opaqueInsets>
</FlowPane>
