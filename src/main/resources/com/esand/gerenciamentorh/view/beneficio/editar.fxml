<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<VBox alignment="TOP_CENTER" minHeight="231.0" minWidth="501.0" prefHeight="217.0" prefWidth="538.0" spacing="15.0" xmlns="http://javafx.com/javafx/17.0.12" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.esand.gerenciamentorh.controller.editar.EditarBeneficioController">
    <children>
        <HBox prefHeight="275.0" prefWidth="644.0">
            <children>
                <VBox alignment="TOP_CENTER" prefHeight="202.0" prefWidth="505.0" spacing="10.0">
                    <children>
                        <GridPane minHeight="150.0" minWidth="475.0" prefHeight="202.0" prefWidth="475.0">
                            <columnConstraints>
                                <ColumnConstraints hgrow="SOMETIMES" maxWidth="95.0" minWidth="10.0" prefWidth="50.0" />
                                <ColumnConstraints fillWidth="false" hgrow="SOMETIMES" maxWidth="400.0" minWidth="10.0" prefWidth="300.0" />
                            </columnConstraints>
                            <rowConstraints>
                                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                            </rowConstraints>
                            <children>
                                <Label text="Tipo" />
                                <TextField fx:id="tipoField" minHeight="26.0" minWidth="181.0" GridPane.columnIndex="1" />
                                <Label text="Descrição" GridPane.rowIndex="1" />
                                <TextField fx:id="descricaoField" minHeight="26.0" minWidth="381.0" GridPane.columnIndex="1" GridPane.rowIndex="1" />
                                <Label text="Valor" GridPane.rowIndex="2" />
                                <TextField fx:id="valorField" minWidth="149.0" GridPane.columnIndex="1" GridPane.rowIndex="2" />
                            </children>
                        </GridPane>
                        <Button mnemonicParsing="false" onAction="#salvar" text="Salvar" />
                        <Label fx:id="errorLabel" textFill="RED" />
                    </children>
                </VBox>
            </children>
        </HBox>
    </children>
    <padding>
        <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
    </padding>
</VBox>
